<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ContactInformationRecordPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>org.jw.service.entity.MeetingPlace</class>
    <class>org.jw.service.entity.Contact</class>
    <class>org.jw.service.entity.ContactStatus</class>
    <class>org.jw.service.entity.ContactCall</class>
    <class>org.jw.service.entity.ServiceGroup</class>
    <class>org.jw.service.entity.Congregation</class>
    <class>org.jw.service.entity.Configuration</class>
    <class>org.jw.service.entity.LocationMap</class>
    <class>org.jw.service.entity.DirectionMap</class>
    <class>org.jw.service.entity.AppsReportParameter</class>
    <class>org.jw.service.entity.PossibleValue</class>
    <class>org.jw.service.entity.AppsReport</class>
    <class>org.jw.service.entity.Territory</class>
    <class>org.jw.service.entity.CallStatus</class>
    <class>org.jw.service.entity.QueryText</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:hsqldb:hsql://localhost"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.jdbc.driver" value="org.hsqldb.jdbcDriver"/>
      <property name="javax.persistence.jdbc.user" value="SA"/>
    </properties>
  </persistence-unit>
</persistence>
